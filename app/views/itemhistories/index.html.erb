<h1>Listing itemhistories</h1>

<table>
  <thead>
    <tr>
      <th>Item</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Distance</th>
      <th>Memo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @itemhistories.each do |itemhistory| %>
      <tr>
        <td><%= itemhistory.item %></td>
        <td><%= itemhistory.start_date %></td>
        <td><%= itemhistory.end_date %></td>
        <td><%= itemhistory.distance %></td>
        <td><%= itemhistory.memo %></td>
        <td><%= link_to 'Show', itemhistory %></td>
        <td><%= link_to 'Edit', edit_itemhistory_path(itemhistory) %></td>
        <td><%= link_to 'Destroy', itemhistory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Itemhistory', new_itemhistory_path %>
